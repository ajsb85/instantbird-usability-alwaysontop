<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>alwaysontop@mic.instantbird.org</em:id>
    <em:name>Always on Top</em:name>
    <em:creator>Benedikt P.</em:creator>
	<em:contributor>Alexander Salas</em:contributor>
    <em:type>2</em:type>
    <em:version>2.0</em:version>
